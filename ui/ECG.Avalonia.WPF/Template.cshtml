@using ECG.Contracts
@inherits RazorViewTemplateEngine.Core.RazorViewPage<TableStructure>
using System;
using System.ComponentModel;
namespace @Model.NameSpace
{
    public class @Model.TableName
    {
        @foreach (var pm in  Model.Columns)
        {
        @:///<summary>
        @:///@pm.Comment
        @:///</summary>
        @:[Description("@pm.Comment")]
 @if(pm.IsCanNull)
 {
   @:public @pm.ColumnType? @pm.ColumnName { get; set;}  = string.Empty;
 }
 else
 {
    @if (pm.ColumnType == "string")
    {   
        @:public @pm.ColumnType @pm.ColumnName { get; set;}  = string.Empty;
    }
    else if (pm.ColumnType == "DateTime")
    {
        @:public @pm.ColumnType @pm.ColumnName { get; set;}  = DateTime.Now;
    }
    else
    {
        @:public @pm.ColumnType @pm.ColumnName { get; set; }
    }
           }
        }
    }
}